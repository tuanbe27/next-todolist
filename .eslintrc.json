{
  "plugins": ["@typescript-eslint"],
  "extends": [
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "plugin:import/recommended",
    "eslint:recommended",
    "plugin:import/typescript"
  ],
  "rules": {
    "prefer-const": 2,
    "@typescript-eslint/no-unused-vars": 2,
    "@typescript-eslint/no-explicit-any": 1
  }
}
